{
  "name": "frontend",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "description": "Vite React frontend for the City Data Analyzer monorepo.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --max-warnings=0",
    "format": "prettier --check \"src/**/*.{ts,tsx,css}\" vite.config.ts vitest.config.ts jest.config.cjs index.html",
    "test": "npm run test:jest && npm run test:vitest",
    "test:jest": "jest --passWithNoTests",
    "test:vitest": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.45",
    "@ai-sdk/google": "^2.0.40",
    "@ai-sdk/openai": "^2.0.71",
    "ai": "^5.0.98",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/jest": "29.5.14",
    "@types/node": "^24.10.1",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom": "25.0.1",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vitest": "2.1.5"
  }
}
